{
  "productName": "sharkplayer",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "sharkplayer",
        "width": 480,
        "height": 320,
        "minWidth": 480,
        "minHeight": 320,
        "maxWidth": 480,
        "maxHeight": 320,
        "transparent": true,
        "theme": "Light",
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
